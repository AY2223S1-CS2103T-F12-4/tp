<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="100.0" spacing="5.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <VBox alignment="CENTER" prefHeight="100.0" prefWidth="100.0">
      <children>
         <HBox alignment="BOTTOM_CENTER" prefHeight="35.0" prefWidth="100.0">
            <children>
            <Label fx:id="id" styleClass="name-label" />
            <Label fx:id="name" styleClass="name-label" text="\$first" />
            </children>
         </HBox>
         <Label id="tutorDescriptionLabel" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="38.0" text="Tutor" textAlignment="CENTER" />
      </children>
   </VBox>
   <FlowPane fx:id="classes" alignment="CENTER_LEFT" />
   <padding>
      <Insets bottom="5.0" right="5.0" />
   </padding>
</HBox>
